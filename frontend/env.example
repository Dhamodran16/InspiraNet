# Frontend Environment Configuration
# Copy this file to .env.local and update the values

# Environment
VITE_ENVIRONMENT=development

# Backend URLs
VITE_BACKEND_URL=http://localhost:5000
VITE_SOCKET_URL=http://localhost:5000
VITE_MEETING_URL=http://localhost:5000

# Frontend URL
VITE_FRONTEND_URL=http://localhost:3000

# Cloudinary Configuration (if using)
VITE_CLOUDINARY_CLOUD_NAME=your_cloud_name
VITE_CLOUDINARY_UPLOAD_PRESET=your_upload_preset

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true
VITE_ENABLE_SERVICE_WORKER=false

# API Configuration
VITE_API_TIMEOUT=30000
VITE_SOCKET_TIMEOUT=20000

# Google API Configuration
# Get these from Google Cloud Console (console.cloud.google.com)
VITE_GOOGLE_CLIENT_ID=9541808845-vhbcd1lqo02s9p10ngfmtfqp3t3rtpfj.apps.googleusercontent.com
VITE_GOOGLE_API_KEY=your_google_api_key_here

# Development Settings
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_SOURCE_MAPS=true
