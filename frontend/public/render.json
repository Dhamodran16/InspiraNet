{
  "routes": [
    {
      "src": "/signin",
      "dest": "/index.html"
    },
    {
      "src": "/signup",
      "dest": "/index.html"
    },
    {
      "src": "/privacy-policy",
      "dest": "/index.html"
    },
    {
      "src": "/terms",
      "dest": "/index.html"
    },
    {
      "src": "/cookie-policy",
      "dest": "/index.html"
    },
    {
      "src": "/team",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/profile",
      "dest": "/index.html"
    },
    {
      "src": "/messages",
      "dest": "/index.html"
    },
    {
      "src": "/notifications",
      "dest": "/index.html"
    },
    {
      "src": "/create-post",
      "dest": "/index.html"
    },
    {
      "src": "/meeting",
      "dest": "/index.html"
    },
    {
      "src": "/meeting/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/profile/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/messages/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": {
    "/**": {
      "Cache-Control": "public, max-age=0, must-revalidate"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/icons/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    }
  },
  "error_page": "index.html"
}
