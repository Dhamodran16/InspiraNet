{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/signin",
      "dest": "/index.html"
    },
    {
      "src": "/signup",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/meeting",
      "dest": "/index.html"
    },
    {
      "src": "/meeting/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/privacy-policy",
      "dest": "/index.html"
    },
    {
      "src": "/terms",
      "dest": "/index.html"
    },
    {
      "src": "/cookie-policy",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

